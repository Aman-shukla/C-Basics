#include<stdio.h>
int fibonacci(int number)
{
    if(number==1||number==2)
    {
        return 1;
    }
    else
    {
        return(fibonacci(number-1)+fibonacci(number-2));
    }
    
}
int main()
{
    int num;
    printf("Enter the number you want fibonacci series of\n");
    scanf("%d",&num);
    printf("The fibonacci series of %d is %d\n",num,fibonacci(num));
    return 0;
}
